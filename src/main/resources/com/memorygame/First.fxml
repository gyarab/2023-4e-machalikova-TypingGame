<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="265.0" prefWidth="398.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.memorygame.FirstController">
   <children>
      <Label fx:id="text_player" layoutX="97.0" layoutY="39.0" prefHeight="30.0" prefWidth="204.0" text="You are the first player">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="text_code" layoutX="67.0" layoutY="91.0" text="Here is your code for multiplayer:" visible="false">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="code" layoutX="169.0" layoutY="144.0" text="       ">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="back" layoutX="174.0" layoutY="209.0" mnemonicParsing="false" onAction="#back" text="Back" />
      <Button fx:id="generator" layoutX="56.0" layoutY="110.0" mnemonicParsing="false" onAction="#generate" text="Generate connection code">
         <font>
            <Font size="22.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
